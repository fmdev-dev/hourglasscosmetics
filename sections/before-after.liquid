<div id="hero-unlocked-2-section" class="section fullheightblock unlocked-2-scn">
  <div class="page-width">
    {% if section.settings.heading != blank %}
    <div class="section-header text-center">
      <h3 class="heading">{{ section.settings.heading | escape }}</h3>
    </div>
    {% endif %}
    <div class="left-section">
      <div class="unlocked-2-container">
        <div class="content-inner">
          <div class="before-after text-left">
            <span class="left">{{section.settings.title1}}</span>
            <span class="left">{{section.settings.title2}}</span>
          </div>
          <div class="before-after-image">
            <span class="left-image">
                <img src="{{ section.settings.image1 | img_url : "100x" }}" data-src="{{ section.settings.image1 | img_url : "master" }}" alt="Hourglass Cosmetics" class="lazyload">
            </span>
            <span class="left-image">
              <img src="{{ section.settings.image2 | img_url : "100x" }}" data-src="{{ section.settings.image2 | img_url : "master" }}" alt="Hourglass Cosmetics" class="lazyload"> 
            </span>        
          </div>
        </div>
      </div>
    </div>
    <div class="right-section">
      <div class="unlocked-2-container">
        <div class="content-inner">      
          <div class="before-after">
            <span class="left">{{section.settings.title1}}</span>
            <span class="left">{{section.settings.title2}}</span>
          </div>
          <div class="before-after-image">
            <span class="left-image">
              <img src="{{ section.settings.image3 | img_url : "100x" }}" data-src="{{ section.settings.image3 | img_url : "master" }}" alt="Hourglass Cosmetics" class="lazyload">
            </span>
            <span class="left-image">
              <img src="{{ section.settings.image4 | img_url : "100x" }}" data-src="{{ section.settings.image4 | img_url : "master" }}" alt="Hourglass Cosmetics" class="lazyload">  
            </span>     
          </div>
        </div>
      </div>
    </div>  
 </div>
</div>




