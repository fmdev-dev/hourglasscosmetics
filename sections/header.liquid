  <script>    
    $( document ).ready(function() {
      var bullet_count=0;
      $("#MainContent .shopify-section").each(function(data) {
          bullet_count = bullet_count+1;
          var section_id = $(this).attr('id');
          var anchor_id = "anchorbullet-"+bullet_count;
          $("#section-bullet").append('<li><a aria-label="section" id="'+anchor_id+'" href="#'+section_id+'">&nbsp;</a></li>');
      }); 
    });    
  </script>
