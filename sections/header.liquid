     jQuery(this).parent().siblings().children('.mobile-nav__icon').removeClass('close-arrow');
    })
    jQuery('header .mobile-nav li child-mobile-nav').click(function(){
     jQuery(this).toggleClass('close-arrow');
      jQuery(this).siblings('ul').stop().toggle()
      jQuery(this).parent().siblings().children('ul').stop().hide();
     jQuery(this).parent().siblings().children('.child-mobile-nav').removeClass('close-arrow');
    })
     
