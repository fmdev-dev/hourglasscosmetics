        function record(){
            percent = Math.round(player.getCurrentTime() / player.getDuration() * 100);

            if(last_percent != percent){
                //console.log('percent 1 : '+percent);
                if( percent == 1 || percent == 25 || percent == 50 || percent == 75 || percent == 100 ){
                    callDataLayerPush( 1, percent, video_title1, video_url1);
