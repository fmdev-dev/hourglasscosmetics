        function callDataLayerPush( no, per, title, url){

            var flag = true;
            if( no == 1 && countTarck1.indexOf(per) !== -1 ){
                flag = false;
            }else if( no == 2 && countTarck2.indexOf(per) !== -1 ){
                flag = false;
            }else if( no == 3 && countTarck3.indexOf(per) !== -1 ){
                flag = false;
            }else if( no == 4 && countTarck4.indexOf(per) !== -1 ){
                flag = false;
            }

            if(flag){
                var videoAction = per;
                if( per == 1 ){
                    videoAction = 'Play';
                }else if ( per == 100 ){
                    videoAction = 'Complete';
                }else{
                    videoAction = per+'%';
                }

                window.dataLayer.push({
                    'event': 'videoTrack',
                    'videoAction': videoAction,
                    'videoName': title,
                    'videoPlayer': 'Youtube',
                    'videoURL': url
                });
                if( no == 1 ){
                    countTarck1.push(per);
                }else if( no == 2 ){
                    countTarck2.push(per);
                }else if( no == 3 ){
                    countTarck3.push(per);
                }else if( no == 4 ){
                    countTarck4.push(per);
                }
            }
        }
       
       
       
