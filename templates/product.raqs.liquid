{%- comment -%}Please DO NOT edit this file{%- endcomment -%}
{%- layout none -%}
{%- assign raqs = shop.metafields.ra_quickshop.settings -%}
{%- assign raqs_enable_zoom_image = raqs.popup.image.zoom -%}
{%- assign raqs_enable_images_gallery = raqs.popup.image.lightbox.enabled -%}
{%- assign raqs_product_image_scale = 2 -%}
{%- assign raqs_product_image_zoom_size = '1024x1024' -%}
{%- assign raqs_image_ratio = raqs.popup.image.ratio -%}
{%- assign current_variant = product.selected_or_first_available_variant -%}
<div class="raqs-single raqs-row raqs-row-0{% if raqs.popup.animation == '1' %} raqs-animation-enabled{% endif %}" data-container>
  <div class="raqs-close"><span class="raqs-close_icon">Ã—</span></div>
  {%- if raqs.popup.image.disabled == '0' -%}
  <div class="raqs-slider-wrapper raqs-col raqs-col-xs-12">
    <div class="raqs-slider{% if raqs.popup.image.thumnails.enabled == '1' %} raqs-thumnails-{{ raqs.popup.image.thumnails.position | escape }}{% if raqs.popup.image.thumnails.vertical == '1' %} raqs-thumnails-vertical{% endif %}{% else %} raqs-thumnails-disabled{% endif %}{% if raqs_enable_zoom_image == '1' %} raqs-zoom-enabled{% endif %}{% if raqs_enable_images_gallery == '1' %} raqs-lightbox-enabled raqs-lightbox-{{ raqs.popup.image.lightbox.position | escape }}{% endif %}" data-product-slider>
      <div class="raqs-images-container">
        <div class="raqs-images">
          {%- assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image -%}
          {%- assign featured_image_aspect_ratio = featured_image.aspect_ratio | default: 1 -%}
          <div class="raqs-image"{% if raqs_enable_zoom_image == '1' %} data-zoom="{{ featured_image | img_url: raqs_product_image_zoom_size, scale: raqs_product_image_scale }}"{% endif %}{% if raqs_enable_images_gallery == '1' %} data-index="0"{% endif %} data-product-image>
            <div class="raqs-ratio raqs-ratio-{{ raqs_image_ratio }}"{% if raqs_image_ratio == '0' %} style="padding-bottom: {{ 100 | divided_by: featured_image_aspect_ratio }}%;"{% endif %}>
              <div class="raqs-ratio-container">
                <div class="raqs-ratio-image raqs-image-img raqs-img-featured"
                     data-index="0"
                     data-image="{{ featured_image | img_url: 'master' }}"
                     data-image_id="{{ featured_image.id }}"
                     data-image_width="{{ featured_image.width }}"
                     data-image_height="{{ featured_image.height }}"
                     style="background-image:url('{{ featured_image | img_url: raqs_product_image_zoom_size }}');"></div>
              </div>
            </div>
          </div>
          {%- assign raqs_index = 1 -%}
          {%- for image in product.images -%}
            {%- unless image == featured_image -%}
              {%- assign image_aspect_ratio = image.aspect_ratio | default: 1 -%}
              <div class="raqs-image"{% if raqs_enable_zoom_image == '1' %} data-zoom="{{ image | img_url: raqs_product_image_zoom_size, scale: raqs_product_image_scale }}"{% endif %}{% if raqs_enable_images_gallery == '1' %} data-index="{{ raqs_index }}"{% endif %} data-product-image>
                <div class="raqs-ratio raqs-ratio-{{ raqs_image_ratio }}"{% if raqs_image_ratio == '0' %} style="padding-bottom: {{ 100 | divided_by: image_aspect_ratio }}%;"{% endif %}>
                  <div class="raqs-ratio-container">
                    <div class="raqs-ratio-image raqs-image-img"
                         data-index="{{ raqs_index }}"
                         data-image="{{ image | img_url: 'master' }}"
                         data-image_id="{{ image.id }}"
                         data-image_width="{{ image.width }}"
                         data-image_height="{{ image.height }}"
                         style="background-image:url('{{ image | img_url: raqs_product_image_zoom_size }}');"></div>
                  </div>
                </div>
              </div>
              {%- assign raqs_index = raqs_index | plus:1 -%}
            {%- endunless -%}
          {%- endfor -%}
        </div>
        {%- unless raqs.popup.catalog.enabled == '1' and raqs.popup.catalog.remove_price == '1' -%}
          {%- if raqs.upsell.sale_badge.label.enabled == '1' -%}
          <div class="raqs-product_price--sale-label at-{{ raqs.upsell.sale_badge.label.position | escape }}{% unless current_variant.available and current_variant.compare_at_price > current_variant.price %} raqs-hide{% endunless %}" style="color: {{ raqs.upsell.sale_badge.label.color | escape }};" data-sale-label>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path fill="{{ raqs.upsell.sale_badge.label.background | escape }}" d="M512 256c0-37.7-23.7-69.9-57.1-82.4 14.7-32.4 8.8-71.9-17.9-98.6-26.7-26.7-66.2-32.6-98.6-17.9C325.9 23.7 293.7 0 256 0s-69.9 23.7-82.4 57.1c-32.4-14.7-72-8.8-98.6 17.9-26.7 26.7-32.6 66.2-17.9 98.6C23.7 186.1 0 218.3 0 256s23.7 69.9 57.1 82.4c-14.7 32.4-8.8 72 17.9 98.6 26.6 26.6 66.1 32.7 98.6 17.9 12.5 33.3 44.7 57.1 82.4 57.1s69.9-23.7 82.4-57.1c32.6 14.8 72 8.7 98.6-17.9 26.7-26.7 32.6-66.2 17.9-98.6 33.4-12.5 57.1-44.7 57.1-82.4z"></path></svg>
            {{- raqs.upsell.sale_badge.label.text -}}
          </div>
          {%- endif -%}
        {%- endunless -%}
      </div>
      {%- if raqs.popup.image.thumnails.enabled == '1' and raqs.popup.image.thumnails.dots == '0' -%}
        <div class="raqs-thumnails-container">
          <div class="raqs-thumbnails">
            <div class="raqs-thumbnail"
                data-thumbnail_id="{{ featured_image.id }}"
                {% if raqs_enable_zoom_image == '1' %}data-zoom="{{ featured_image.src | img_url: raqs_product_image_zoom_size, scale: raqs_product_image_scale }}"{% endif %} data-product-thumbs>
              <div class="raqs-ratio raqs-ratio-1">
                <div class="raqs-ratio-container">
                  <div class="raqs-ratio-image raqs-thumbnail-img" style="background-image:url('{{ featured_image | img_url: '100x' }}');"></div>
                </div>
              </div>
            </div>

            {%- for image in product.images -%}
              {%- unless image == featured_image -%}
                <div class="raqs-thumbnail"
                    data-thumbnail_id="{{ image.id }}"
                    {% if raqs_enable_zoom_image == '1' %}data-zoom="{{ image.src | img_url: raqs_product_image_zoom_size, scale: raqs_product_image_scale }}"{% endif %} data-product-thumbs>
                  <div class="raqs-ratio raqs-ratio-1">
                    <div class="raqs-ratio-container">
                      <div class="raqs-ratio-image raqs-thumbnail-img" style="background-image:url('{{ image | img_url: '100x' }}');"></div>
                    </div>
                  </div>
                </div>
              {%- endunless -%}
            {%- endfor -%}
          </div>
        </div>
      {%- endif -%}
      <ul class="raqs-controls{% if raqs.popup.image.controls != '1' %} raqs-hide{% endif %}">
        <li class="raqs-control raqs-prev">
          <svg class="raqs-svg" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100"><g><path d="M 10,50 L 60,100 L 60,90 L 20,50  L 60,10 L 60,0 Z"></path></g></svg>
        </li>
        <li class="raqs-control raqs-next">
          <svg class="raqs-svg" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100"><g><path d="M 10,50 L 60,100 L 60,90 L 20,50  L 60,10 L 60,0 Z" transform="translate(100,100) rotate(180)"></path></g></svg>
        </li>
      </ul>
      {%- if raqs.popup.image.lightbox.enabled == '1' -%}
      <div class="raqs-expand">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 97.309 97.308"><path d="M97.153,87.8l-4.736-17.672c-0.181-0.675-0.707-1.201-1.382-1.383c-0.673-0.181-1.396,0.013-1.889,0.507l-3.098,3.097   L70.766,60.194V37.115l15.283-12.156l3.098,3.097c0.493,0.493,1.216,0.687,1.889,0.506c0.675-0.181,1.201-0.708,1.382-1.383   l4.807-17.933c0.183-0.674-0.012-1.394-0.506-1.888c-0.494-0.495-1.214-0.686-1.888-0.506l-17.938,4.801   c-0.674,0.181-1.2,0.707-1.382,1.382c-0.182,0.675,0.013,1.395,0.506,1.889l3.101,3.099L66.708,33.624H30.584L18.176,18.022   l3.098-3.099c0.495-0.494,0.688-1.215,0.506-1.889c-0.18-0.675-0.708-1.201-1.382-1.382l-17.936-4.8   c-0.677-0.18-1.395,0.011-1.889,0.506C0.079,7.852-0.114,8.572,0.067,9.246l4.808,17.933c0.18,0.675,0.707,1.202,1.382,1.383   c0.673,0.181,1.395-0.013,1.888-0.506l3.098-3.097l15.284,12.157v23.077L11.242,72.349l-3.098-3.096   c-0.493-0.493-1.215-0.688-1.888-0.507c-0.675,0.182-1.202,0.708-1.382,1.383L0.067,88.062c-0.181,0.674,0.012,1.394,0.506,1.888   c0.372,0.371,0.871,0.573,1.383,0.573c0.169,0,0.339-0.021,0.506-0.067l17.937-4.801c0.674-0.181,1.202-0.707,1.382-1.382   c0.182-0.674-0.011-1.395-0.506-1.889l-3.098-3.1l12.408-15.601h36.122l12.408,15.602l-3.101,3.1   c-0.493,0.494-0.688,1.214-0.506,1.889s0.708,1.201,1.383,1.382l17.938,4.8c0.167,0.046,0.336,0.067,0.505,0.067   c0.007,0,0.016,0,0.02,0c1.081,0,1.954-0.876,1.954-1.955C97.308,88.295,97.253,88.037,97.153,87.8z"></path></svg>
        {%- if raqs.popup.image.lightbox.label != blank -%}
        <label>{{ raqs.popup.image.lightbox.label }}</label>
        {%- endif -%}
      </div>
      {%- endif -%}
    </div>
  </div>
  {%- endif -%}
  <div class="raqs-info-wrapper raqs-col{% if raqs.popup.image.disabled == '1' %} raqs-col-12{% endif %} raqs-col-xs-12">
    <div class="raqs-info-container">
      <div class="raqs-info">
        <div class="raqs-info_top">
          <h2 class="raqs-product_title raqs-size-{{ raqs.popup.title_size | escape }}">{{ product.title }}</h2>
        </div>
        <div class="raqs-info_middle">
          {%- if raqs.popup.review != "" -%}
          <div class="raqs-product_review">
            {%- case raqs.popup.review -%}
              {%- when 'product-reviews' -%}
              <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>
            
              {%- when 'judgeme' -%}
              {%- include 'judgeme_widgets', widget_type: 'judgeme_preview_badge', jm_style: '', concierge_install: true -%}
            
              {%- when 'loox' -%}
              <div class="loox-rating" data-id="{{ product.id }}" data-rating="{{ product.metafields.loox.avg_rating }}" data-raters="{{ product.metafields.loox.num_reviews }}"></div>
            {%- endcase -%}
          </div>
          {%- endif -%}
          {%- unless raqs.popup.catalog.enabled == '1' and raqs.popup.catalog.remove_price == '1' -%}
            {%- if product.price_varies -%}
            <div class="raqs-product_price_varies">
              <span class="raqs-product_price--regular"><span class="raqs-product_price--display"><span class="money">{{ product.price_min | money }}</span></span></span>
              {{- '&nbsp;&#8212;&nbsp;' -}}
              <span class="raqs-product_price--regular"><span class="raqs-product_price--display"><span class="money">{{ product.price_max | money }}</span></span></span>
            </div>
            {%- endif -%}
          {%- endunless -%}
          {%- if product.available and raqs.upsell.total_sales.enabled == '1' -%}
            <div class="raqs-total_sales raqs-hide" data-product_id="{{ product.id | escape }}" data-minimum_qty="{{ raqs.upsell.total_sales.minimum_qty | escape }}" data-maximum_qty="{{ raqs.upsell.total_sales.maximum_qty | escape }}" data-minimum_time="{{ raqs.upsell.total_sales.minimum_time | escape }}" data-maximum_time="{{ raqs.upsell.total_sales.maximum_time | escape }}" style="color: {{ raqs.upsell.total_sales.color | escape }};">
              {%- assign divided_value = raqs.upsell.total_sales.minimum_qty | plus:raqs.upsell.total_sales.maximum_qty | divided_by:2 | round -%}
              {%- assign new_value = "<strong>" | append: divided_value | append:"</strong>" -%}
              {%- assign search_value = "{{ count }}" -%}
              {%- assign divided_value2 = raqs.upsell.total_sales.minimum_time | plus:raqs.upsell.total_sales.maximum_time | divided_by:2 | round -%}
              {%- assign new_value2 = "<time>" | append: divided_value2 | append:"</time>" -%}
              {%- assign search_value2 = "{{ time }}" -%}
              {{- raqs.upsell.total_sales.message | replace: search_value, new_value | replace: search_value2, new_value2 -}}
            </div>
          {%- endif -%}

          {%- if product.available and raqs.upsell.stock_countdown.enabled == '1' -%}
            {%- assign current_variant_available = false -%}
            {%- assign stock_countdown_simulation = false -%}

            {%- if raqs.upsell.stock_countdown.simulation.enabled != '1' -%}
              {%- if current_variant.available and current_variant.inventory_quantity > 0 and current_variant.inventory_management == 'shopify' -%}
                {%- assign current_variant_available = true -%}
              {%- endif -%}
            {%- else -%}
              {%- if current_variant.available -%}
                {%- assign current_variant_available = true -%}
                {%- if current_variant.inventory_quantity <= 0 and current_variant.inventory_management == 'shopify' and current_variant.inventory_policy == 'deny' -%}
                  {%- assign current_variant_available = false -%}
                {%- endif -%}
              {%- endif -%}
              {%- assign stock_countdown_simulation = true -%}
              {%- assign stock_countdown_simulation_minimum = raqs.upsell.stock_countdown.simulation.minimum -%}
              {%- assign stock_countdown_simulation_maximum = raqs.upsell.stock_countdown.simulation.maximum -%}
            {%- endif -%}

            {%- if current_variant_available or stock_countdown_simulation -%}
            <div class="raqs-stock_countdown{% if stock_countdown_simulation and current_variant_available %} raqs-hide{% endif %}" data-product_id="{{ product.id | escape }}" data-minimum="{{ stock_countdown_simulation_minimum | escape }}" data-maximum="{{ stock_countdown_simulation_maximum | escape }}">
              <div class="raqs-stock_countdown-message">
                {%- assign inventory_quantity = current_variant.inventory_quantity | plus:1 -%}
                {%- assign new_value = "<strong>" | append: inventory_quantity | append:"</strong>" -%}
                {%- assign search_value = "{{ count }}" -%}
                {{- raqs.upsell.stock_countdown.message | replace: search_value, new_value -}}
              </div>
              <div class="raqs-stock_countdown-progress" style="background-color: {{ raqs.upsell.stock_countdown.background | escape }};">
                <span style="background-color: {{ raqs.upsell.stock_countdown.color | escape }};"></span>
              </div>
            </div>
            {%- endif -%}
          {%- endif -%}

          {%- unless raqs.popup.desc_height == '0' -%}
          <div class="raqs-product_short">
            <div class="raqs-product_short--main">{{ product.description }}</div>
            <div class="raqs-product_short--footer">
              <span class="raqs-product_short--showmore">
                {{- raqs.texts.show_more -}}
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M151.5 347.8L3.5 201c-4.7-4.7-4.7-12.3 0-17l19.8-19.8c4.7-4.7 12.3-4.7 17 0L160 282.7l119.7-118.5c4.7-4.7 12.3-4.7 17 0l19.8 19.8c4.7 4.7 4.7 12.3 0 17l-148 146.8c-4.7 4.7-12.3 4.7-17 0z"></path></svg>
              </span>
              <span class="raqs-product_short--showless">
                {{- raqs.texts.show_less -}}
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M168.5 164.2l148 146.8c4.7 4.7 4.7 12.3 0 17l-19.8 19.8c-4.7 4.7-12.3 4.7-17 0L160 229.3 40.3 347.8c-4.7 4.7-12.3 4.7-17 0L3.5 328c-4.7-4.7-4.7-12.3 0-17l148-146.8c4.7-4.7 12.3-4.7 17 0z"></path></svg>
              </span>
            </div>
          </div>
          {%- endunless -%}

          {%- if raqs.popup.view_more == '1' -%}
          <div class="raqs-product_readmore">
            <a href="{{ product.url }}">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><path d="M311.03 131.515l-7.071 7.07c-4.686 4.686-4.686 12.284 0 16.971L387.887 239H12c-6.627 0-12 5.373-12 12v10c0 6.627 5.373 12 12 12h375.887l-83.928 83.444c-4.686 4.686-4.686 12.284 0 16.971l7.071 7.07c4.686 4.686 12.284 4.686 16.97 0l116.485-116c4.686-4.686 4.686-12.284 0-16.971L328 131.515c-4.686-4.687-12.284-4.687-16.97 0z"></path></svg>
              {{- raqs.texts.product_details -}}
            </a>
          </div>
          {%- endif -%}
        </div>
        <div class="raqs-info_bottom">
          {%- if product.available == false -%}
            <div class="raqs-product_incoming">
              {%- if current_variant.incoming -%}
                {%- assign new_value = current_variant.next_incoming_date | date: raqs.texts.date_format -%}
                {%- assign search_value = "{{ date }}" -%}
                {{- raqs.texts.incoming | replace: search_value, new_value -}}
              {%- else -%}
                {{- raqs.texts.sold_out -}}
              {%- endif -%}
            </div>
            {%- if raqs.upsell.notify_me.enabled == '1' -%}
              <div class="raqs-notify_me">
                {%- form 'contact', id: 'raqs-notify_me' -%}
                  {%- unless form.errors == blank -%}
                  <div class="alert__message">
                    <div class="alert__icon"><i class="ris ri-alert"></i></div>
                    <div class="alert__content">{{ 'products.stock_notification_form.error' | t }}</div>
                  </div>
                  {%- endunless -%}
                  {%- if form.posted_successfully? -%}
                  <div class="alert__message">
                    <div class="alert__icon"><i class="ris ri-check"></i></div>
                    <div class="alert__content">{{ 'products.stock_notification_form.success' | t }}</div>
                  </div>
                  {%- endif -%}

                  <label for="raqs-notify_me--title">{{ raqs.upsell.notify_me.texts.title }}</label>
                  <div class="raqs-notify_me--group raqs-row">
                    <div class="raqs-col raqs-col-auto">
                      <input id="raqs-notify_me--title" class="raqs-notify_me--title" type="email" name="contact[email]" required="required" value="{% if customer %}{{ customer.email }}{% else %}{{ contact.fields.email }}{% endif %}" placeholder="{{ raqs.upsell.notify_me.texts.placeholder | escape }}" autocapitalize="off" />
                    </div>
                    <div class="raqs-col raqs-col-auto">
                      <button type="submit" class="raqs-notify_me--button">{{ raqs.upsell.notify_me.texts.button }}</button>
                    </div>
                  </div>
                  {%- assign new_value = product.title -%}
                  {%- assign search_value = "{{ product.title }}" -%}
                  <input type="hidden" name="contact[body]" value="{{ raqs.upsell.notify_me.texts.body | replace: search_value, new_value | escape }}" />
                  <input type="hidden" name="contact[product_title]" value="{{ product.title | escape }}" />
                  <input type="hidden" name="contact[product_url]" value="{{ shop.url }}{{ product.url | escape }}" />
                {%- endform -%}
              </div>
            {%- endif -%}
          {%- else -%}
            {%- form 'product', product, class: 'raqs-product_form', novalidate: 'novalidate', data-product-form: '' -%}
              {%- unless product.has_only_default_variant -%}
              <div class="raqs-selectors">
                {%- for product_option in product.options_with_values -%}
                  {%- assign forloop_index0 = forloop.index0 -%}
                  {%- assign forloop_index = forloop.index -%}
                  {%- assign is_color = false -%}
                  {%- assign downcased_product_option = product_option.name | handle -%}
                  {%- if downcased_product_option contains 'color' or downcased_product_option contains 'colour' -%}
                    {%- assign is_color = true -%}
                  {%- endif -%}
                  <div class="raqs-selector-wrapper js">
                    <label for="SingleOptionSelector-{{ forloop_index0 }}"{% if product_option.name == 'default' or raqs.popup.label_disabled == '1' %} class="raqs-hide"{% endif %}>{{ product_option.name }}</label>
                    <div class="raqs-single-option-selector-wrapper">
                      {%- unless raqs.popup.swatches == '1' -%}
                        <select class="raqs-single-option-selector" id="SingleOptionSelector-{{ forloop_index0 }}" data-index="option{{ forloop_index }}">
                          {%- for value in product_option.values -%}
                          <option value="{{ value | escape }}"{% if product_option.selected_value == value %} selected="selected"{% endif %}>{{ value }}</option>
                          {%- endfor -%}
                        </select>
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M151.5 347.8L3.5 201c-4.7-4.7-4.7-12.3 0-17l19.8-19.8c4.7-4.7 12.3-4.7 17 0L160 282.7l119.7-118.5c4.7-4.7 12.3-4.7 17 0l19.8 19.8c4.7 4.7 4.7 12.3 0 17l-148 146.8c-4.7 4.7-12.3 4.7-17 0z"></path></svg>
                      {%- else -%}
                        {%- for value in product_option.values -%}
                          {%- assign bg_img = '' -%}
                          {%- if is_color and raqs.popup.swatches_advanced == '1' -%}
                            {%- for variant in product.variants -%}
                              {%- for option in variant.options -%}
                                {%- if option == value and variant.image.src != '' -%}
                                  {%- assign bg_img = variant.image.src -%}
                                {%- endif -%}
                              {%- endfor -%}
                            {%- endfor -%}
                          {%- endif -%}
                          <label class="raqs-single-option-selector-container {{ downcased_product_option }}{% if bg_img != '' %} image{% endif %}" for="SingleOptionSelector-{{ forloop_index0 }}{{ forloop.index0 }}">
                            <input class="raqs-single-option-selector" id="SingleOptionSelector-{{ forloop_index0 }}{{ forloop.index0 }}" type="radio" name="option{{ forloop_index }}" data-index="option{{ forloop_index }}" value="{{ value | escape }}"{% if product_option.selected_value == value %} checked="checked"{% endif %} />
                            <ins{% if is_color %} style="background-color: {{ value | split:' ' | last | handle }}; background-image: url({% if bg_img != '' %}{{ bg_img | img_url: 'small' }}{% else %}{{ value | handle | append:'.png' | asset_url }}{% endif %});"{% endif %}><span>{{ value }}</span></ins>
                          </label>
                        {%- endfor -%}
                      {%- endunless -%}
                    </div>
                  </div>
                {%- endfor -%}
              </div>
              {%- endunless -%}
              {%- unless raqs.popup.catalog.enabled == '1' and raqs.popup.catalog.remove_price == '1' -%}
              <div class="raqs-product_price{% if current_variant.available and current_variant.compare_at_price > current_variant.price %} raqs-product_price--on-sale{% if raqs.upsell.sale_badge.saved.enabled == '1' %} raqs-product_price--saved-available{% endif %}{% endif %}" data-price>
                <div class="raqs-product_price--main">
                  <del class="raqs-product_price--sale"><span class="raqs-product_price--display" data-sale-price><span class="money">{{ current_variant.compare_at_price | money }}</span></span></del>
                  <span class="raqs-product_price--regular"><span class="raqs-product_price--display" data-regular-price><span class="money">{{ current_variant.price | money }}</span></span></span>
                </div>
                {%- if raqs.upsell.sale_badge.saved.enabled == '1' -%}
                  {%- capture raqs_saved_amount -%}<span class="money">{{ current_variant.compare_at_price | minus:current_variant.price | money }}</span>{%- endcapture -%}
                  {%- if raqs.upsell.sale_badge.saved.percentage == '1' -%}
                    {%- capture raqs_saved_amount -%}{% if current_variant.compare_at_price > current_variant.price %}{{ current_variant.compare_at_price | minus:current_variant.price | times:100.00 | divided_by:current_variant.compare_at_price | ceil }}{% endif %}%{%- endcapture -%}
                  {%- endif -%}
                <span class="raqs-product_price--saved" style="color: {{ raqs.upsell.sale_badge.saved.color | escape }}; border-color: {{ raqs.upsell.sale_badge.saved.color | escape }};">{{ raqs.upsell.sale_badge.saved.text }}&nbsp;<span class="raqs-product_price--display" data-saved-amount>{{ raqs_saved_amount }}</span><span class="raqs-product_price--saved-background" style="background-color: {{ raqs.upsell.sale_badge.saved.color | escape }};"></span></span>
                {%- endif -%}
              </div>
              {%- endunless -%}
              <select name="id" id="raqs-ProductSelect" class="raqs-hide">
                {%- for variant in product.variants -%}
                  {%- if variant.available -%}
                    <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} value="{{ variant.id }}">{{- variant.title -}}</option>
                  {%- else -%}
                    <option disabled="disabled">{{ variant.title }} - {{ raqs.texts.sold_out }}</option>
                  {%- endif -%}
                {%- endfor -%}
              </select>
              <div class="raqs-row" data-add-to-cart-wrap>
                {%- if raqs.popup.catalog.enabled != '1' -%}
                <div class="raqs-col raqs-col-auto raqs-col-sm-12">
                  <div class="raqs-product_group raqs-row">
                    {%- unless raqs.popup.qty_disabled == '1' -%}
                    <div class="raqs-product_qty raqs-col raqs-col-auto{% unless current_variant.available %} raqs-disabled{% endunless %}">
                      <button type="button" class="raqs-product_qty--minus"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512"><path d="M376 232H8c-4.42 0-8 3.58-8 8v32c0 4.42 3.58 8 8 8h368c4.42 0 8-3.58 8-8v-32c0-4.42-3.58-8-8-8z"></path></svg></button>
                      <input type="number" name="quantity" value="1" min="1" class="raqs-product_qty--input" pattern="[0-9]*" />
                      <button type="button" class="raqs-product_qty--plus"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 384 512"><path d="M376 232H216V72c0-4.42-3.58-8-8-8h-32c-4.42 0-8 3.58-8 8v160H8c-4.42 0-8 3.58-8 8v32c0 4.42 3.58 8 8 8h160v160c0 4.42 3.58 8 8 8h32c4.42 0 8-3.58 8-8V280h160c4.42 0 8-3.58 8-8v-32c0-4.42-3.58-8-8-8z"></path></svg></button>
                    </div>
                    {%- endunless -%}
                    <div class="raqs-product_add raqs-col raqs-col-auto">
                      <button type="submit" name="add"{% unless current_variant.available %} disabled="disabled"{% endunless %} class="raqs-product_add--submit" data-add-to-cart>
                        <span class="raqs-product_add--text" data-sold_out="{{ raqs.texts.sold_out | escape }}" data-unavailable="{{ raqs.texts.unavailable | escape }}" data-pre_order="{{ raqs.texts.pre_order | escape }}" data-add_to_cart="{{ raqs.texts.add_to_cart | escape }}" data-add-to-cart-text>
                          {%- unless current_variant.available -%}
                            {{- raqs.texts.sold_out -}}
                          {%- else -%}
                            {%- if raqs.popup.pre_order == '1' and current_variant.inventory_quantity <= 0 and current_variant.inventory_management == 'shopify' -%}
                              {{- raqs.texts.pre_order -}}
                            {%- else -%}
                              {{- raqs.texts.add_to_cart -}}
                            {%- endif -%}
                          {%- endunless -%}
                        </span>
                        <span class="raqs-product_add--progress"></span>
                        <span class="raqs-product_add--checked">
                          <svg class="raqs-svg-check" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><path d="M413.505 91.951L133.49 371.966l-98.995-98.995c-4.686-4.686-12.284-4.686-16.971 0L6.211 284.284c-4.686 4.686-4.686 12.284 0 16.971l118.794 118.794c4.686 4.686 12.284 4.686 16.971 0l299.813-299.813c4.686-4.686 4.686-12.284 0-16.971l-11.314-11.314c-4.686-4.686-12.284-4.686-16.97 0z"></path></svg>
                          <svg class="raqs-svg-exclamation" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M256 40c118.621 0 216 96.075 216 216 0 119.291-96.61 216-216 216-119.244 0-216-96.562-216-216 0-119.203 96.602-216 216-216m0-32C119.043 8 8 119.083 8 256c0 136.997 111.043 248 248 248s248-111.003 248-248C504 119.083 392.957 8 256 8zm-11.49 120h22.979c6.823 0 12.274 5.682 11.99 12.5l-7 168c-.268 6.428-5.556 11.5-11.99 11.5h-8.979c-6.433 0-11.722-5.073-11.99-11.5l-7-168c-.283-6.818 5.167-12.5 11.99-12.5zM256 340c-15.464 0-28 12.536-28 28s12.536 28 28 28 28-12.536 28-28-12.536-28-28-28z"></path></svg>
                        </span>
                      </button>
                    </div>
                  </div>
                </div>
                {%- endif -%}
                {%- if raqs.popup.catalog.enabled != '1' and raqs.popup.payment_button == '1' -%}
                <div class="raqs-col raqs-col-auto raqs-col-sm-12">
                  <div class="raqs-product_buy1click">
                    {{- form | payment_button -}}
                  </div>
                </div>
                {%- endif -%}
              </div>
              <div class="raqs-product_alert raqs-hide" data-form-alert></div>
              <div class="raqs-product_message raqs-hide" data-sold_out="{{ raqs.texts.sold_out | escape }}" data-unavailable="{{ raqs.texts.unavailable | escape }}" data-form-message></div>
            {%- endform -%}

            {%- if raqs.upsell.visitor_counter.enabled == '1' -%}
            <div class="raqs-visitor_counter raqs-hide" data-maximum="{{ raqs.upsell.visitor_counter.maximum | escape }}" data-interval="{{ raqs.upsell.visitor_counter.interval | escape }}" style="color: {{ raqs.upsell.visitor_counter.color | escape }};">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 640 512"><path d="M544 224c44.2 0 80-35.8 80-80s-35.8-80-80-80-80 35.8-80 80 35.8 80 80 80zm0-128c26.5 0 48 21.5 48 48s-21.5 48-48 48-48-21.5-48-48 21.5-48 48-48zM320 256c61.9 0 112-50.1 112-112S381.9 32 320 32 208 82.1 208 144s50.1 112 112 112zm0-192c44.1 0 80 35.9 80 80s-35.9 80-80 80-80-35.9-80-80 35.9-80 80-80zm244 192h-40c-15.2 0-29.3 4.8-41.1 12.9 9.4 6.4 17.9 13.9 25.4 22.4 4.9-2.1 10.2-3.3 15.7-3.3h40c24.2 0 44 21.5 44 48 0 8.8 7.2 16 16 16s16-7.2 16-16c0-44.1-34.1-80-76-80zM96 224c44.2 0 80-35.8 80-80s-35.8-80-80-80-80 35.8-80 80 35.8 80 80 80zm0-128c26.5 0 48 21.5 48 48s-21.5 48-48 48-48-21.5-48-48 21.5-48 48-48zm304.1 180c-33.4 0-41.7 12-80.1 12-38.4 0-46.7-12-80.1-12-36.3 0-71.6 16.2-92.3 46.9-12.4 18.4-19.6 40.5-19.6 64.3V432c0 26.5 21.5 48 48 48h288c26.5 0 48-21.5 48-48v-44.8c0-23.8-7.2-45.9-19.6-64.3-20.7-30.7-56-46.9-92.3-46.9zM480 432c0 8.8-7.2 16-16 16H176c-8.8 0-16-7.2-16-16v-44.8c0-16.6 4.9-32.7 14.1-46.4 13.8-20.5 38.4-32.8 65.7-32.8 27.4 0 37.2 12 80.2 12s52.8-12 80.1-12c27.3 0 51.9 12.3 65.7 32.8 9.2 13.7 14.1 29.8 14.1 46.4V432zM157.1 268.9c-11.9-8.1-26-12.9-41.1-12.9H76c-41.9 0-76 35.9-76 80 0 8.8 7.2 16 16 16s16-7.2 16-16c0-26.5 19.8-48 44-48h40c5.5 0 10.8 1.2 15.7 3.3 7.5-8.5 16.1-16 25.4-22.4z"></path></svg>
              {%- assign divided_value = raqs.upsell.visitor_counter.maximum | divided_by:2 | round -%}
              {%- assign new_value = "<strong>" | append: divided_value | append:"</strong>" -%}
              {%- assign search_value = "{{ count }}" -%}
              {{- raqs.upsell.visitor_counter.message | replace: search_value, new_value -}}
            </div>
            {%- endif -%}
          
            {%- if raqs.upsell.share_buttons.enabled == '1' -%}
              {%- if raqs.upsell.share_buttons.facebook.enabled == '1' or raqs.upsell.share_buttons.twitter.enabled == '1' or raqs.upsell.share_buttons.pinterest.enabled == '1' -%}
                <ul class="raqs-share_buttons raqs-product_metas">
                  <li>
                    {%- assign share_buttons_first = true -%}
                    {%- if raqs.upsell.share_buttons.facebook.enabled == '1' -%}
                      {%- if share_buttons_first == false -%},&nbsp;{%- endif -%}
                      <a href="https://www.facebook.com/sharer.php?u={{ shop.url }}{{ product.url }}" target="_blank">{{ raqs.upsell.share_buttons.facebook.label }}</a>
                      {%- assign share_buttons_first = false -%}
                    {%- endif -%}
                    {%- if raqs.upsell.share_buttons.twitter.enabled == '1' -%}
                      {%- if share_buttons_first == false -%},&nbsp;{%- endif -%}
                      <a href="https://twitter.com/home?status={{ product.title | escape }}%20{{ shop.url }}{{ product.url }}" target="_blank">{{ raqs.upsell.share_buttons.twitter.label }}</a>
                      {%- assign share_buttons_first = false -%}
                    {%- endif -%}
                    {%- if raqs.upsell.share_buttons.pinterest.enabled == '1' -%}
                      {%- if share_buttons_first == false -%},&nbsp;{%- endif -%}
                      <a href="https://pinterest.com/pin/create/button/?url={{ shop.url }}{{ product.url }}&amp;media={{ product.featured_image | img_url: 'master' }}&amp;description={{ product.title | escape }}" target="_blank">{{ raqs.upsell.share_buttons.pinterest.label }}</a>
                      {%- assign share_buttons_first = false -%}
                    {%- endif -%}
                  </li>
                </ul>
              {%- endif -%}
            {%- endif -%}

            {%- if raqs.popup.sku == '1' or raqs.popup.type == '1' or raqs.popup.vendor == '1' or raqs.popup.collections == '1' or raqs.popup.tags == '1' -%}
            <ul class="raqs-product_metas">
              {%- if raqs.popup.sku == '1' -%}
              <li>{{ raqs.texts.sku }}: <span class="raqs-product_meta-sku" data-variant-sku>{{ current_variant.sku | default: 'N/A' }}</span></li>
              {%- endif -%}
              {%- if raqs.popup.type == '1' -%}
              <li>{{ raqs.texts.type }}: <span class="raqs-product_meta-type">{{ product.vendor | link_to_type }}</span></li>
              {%- endif -%}
              {%- if raqs.popup.vendor == '1' -%}
              <li>{{ raqs.texts.vendor }}: <span class="raqs-product_meta-vendor">{{ product.vendor | link_to_vendor }}</span></li>
              {%- endif -%}
              {%- if raqs.popup.collections == '1' and product.collections.size > 0 -%}
              <li>{{ raqs.texts.collections }}:
                <span class="raqs-product_meta-collections">
                  {%- for category in product.collections -%}
                    {% if forloop.index0 > 0 and forloop.index0 < product.collections.size %}, {% endif %}<a href="{{ category.url }}" title="{{ category.title | escape }}">{{ category.title }}</a>
                  {%- endfor -%}
                </span>
              </li>
              {%- endif -%}
              {%- if raqs.popup.tags == '1' and product.tags.size > 0 -%}
              <li>{{ raqs.texts.tags }}:
                <span class="raqs-product_meta-tags">
                  {%- for tag in product.tags -%}
                    {% if forloop.index0 > 0 and forloop.index0 < product.tags.size %}, {% endif %}<a href="{{ routes.all_products_collection_url  }}/{{ tag | handle }}" title="{{ tag }}">{{ tag | capitalize }}</a>
                  {%- endfor -%}
                </span>
              </li>
              {%- endif -%}
            </ul>
            {%- endif -%}
          {%- endif -%}
          {%- if raqs.upsell.recommendations.enabled == '1' -%}
          <div class="raqs-recommendations raqs-hide" data-product_id="{{ product.id }}">
            {%- if raqs.upsell.recommendations.heading != blank -%}
            <h3 class="raqs-recommendations_heading">{{ raqs.upsell.recommendations.heading }}</h3>
            {%- endif -%}
            <ul class="raqs-recommendations_content"></ul>
          </div>
          {%- endif -%}
        </div>
      </div>
    </div>
  </div>
</div>
<script type="application/json" id="raqs-ProductJson">{{ product | json }}</script>

{%- if raqs.upsell.announcement.enabled == '1' -%}
<div class="raqs-announcement raqs-is_closed" style="color: {{ raqs.upsell.announcement.color | escape }}; background-color: {{ raqs.upsell.announcement.background | color_modify: 'alpha', 0.95 }};">
  <div class="raqs-announcement_content">
    <div class="raqs-announcement_message">
      {%- if raqs.upsell.announcement.link != blank -%}
      <a href="{{ raqs.upsell.announcement.link | escape }}" class="raqs-announcement_link">
        {%- endif -%}
        {{- raqs.upsell.announcement.message -}}
        {%- if raqs.upsell.announcement.link != blank -%}
      </a>
      {%- endif -%}
    </div>
  </div>
  <div class="raqs-announcement_toggle">
    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 320 512"><path d="M193.94 256L296.5 153.44l21.15-21.15c3.12-3.12 3.12-8.19 0-11.31l-22.63-22.63c-3.12-3.12-8.19-3.12-11.31 0L160 222.06 36.29 98.34c-3.12-3.12-8.19-3.12-11.31 0L2.34 120.97c-3.12 3.12-3.12 8.19 0 11.31L126.06 256 2.34 379.71c-3.12 3.12-3.12 8.19 0 11.31l22.63 22.63c3.12 3.12 8.19 3.12 11.31 0L160 289.94 262.56 392.5l21.15 21.15c3.12 3.12 8.19 3.12 11.31 0l22.63-22.63c3.12-3.12 3.12-8.19 0-11.31L193.94 256z"></path></svg>
    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" style="background-color: {{ raqs.upsell.announcement.background | color_modify: 'alpha', 0.95 }};"><path d="M443.5 98.5l-211 211.1c-4.7 4.7-12.3 4.7-17 0L4.5 98.5c-4.7-4.7-4.7-12.3 0-17l7.1-7.1c4.7-4.7 12.3-4.7 17 0L224 269.9 419.5 74.5c4.7-4.7 12.3-4.7 17 0l7.1 7.1c4.6 4.6 4.6 12.2-.1 16.9zm0 111l-7.1-7.1c-4.7-4.7-12.3-4.7-17 0L224 397.9 28.5 202.5c-4.7-4.7-12.3-4.7-17 0l-7.1 7.1c-4.7 4.7-4.7 12.3 0 17l211 211.1c4.7 4.7 12.3 4.7 17 0l211-211.1c4.8-4.8 4.8-12.4.1-17.1z"></path></svg>
  </div>
</div>
{%- endif -%}